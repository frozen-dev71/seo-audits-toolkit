{% extends "base.jinja2" %} {% block title %}Extract Image Page{% endblock %} {% block list %}
<li class="breadcrumb-item"><a href="/extract">Extract</a></li>
<li class="breadcrumb-item"><a href="/extract/images">Images</a></li>
<li class="breadcrumb-item active" aria-current="page">{{id}}</a>
</li>
{%endblock%} {% block content %}

<div class="row">
    <div class="col-xl-3 col-md-6">
        <div class="card card-stats">
            <!-- Card body -->
            <div class="card-body">
                <div class="row">
                    <div class="col">
                        <h5 class="card-title text-uppercase text-muted mb-0"> Missing Title</h5>
                        <span class="h2 font-weight-bold mb-0">{{missing_title}}</span>
                    </div>
                    <div class="col-auto">
                        <div class="icon icon-shape bg-gradient-red text-white rounded-circle shadow">
                            <i class="ni ni-active-40"></i>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <div class="col-xl-3 col-md-6">
        <div class="card card-stats">
            <!-- Card body -->
            <div class="card-body">
                <div class="row">
                    <div class="col">
                        <h5 class="card-title text-uppercase text-muted mb-0">Missing Alt</h5>
                        <span class="h2 font-weight-bold mb-0">{{missing_alt}}</span>
                    </div>
                    <div class="col-auto">
                        <div class="icon icon-shape bg-gradient-orange text-white rounded-circle shadow">
                            <i class="ni ni-chart-pie-35"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xl-3 col-md-6">
        <div class="card card-stats">
            <!-- Card body -->
            <div class="card-body">
                <div class="row">
                    <div class="col">
                        <h5 class="card-title text-uppercase text-muted mb-0">Duplicates</h5>
                        <span class="h2 font-weight-bold mb-0">{{duplicates}}</span>
                    </div>
                    <div class="col-auto">
                        <div class="icon icon-shape bg-gradient-green text-white rounded-circle shadow">
                            <i class="ni ni-money-coins"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xl-3 col-md-6">
        <div class="card card-stats">
            <!-- Card body -->
            <div class="card-body">
                <div class="row">
                    <div class="col">
                        <h5 class="card-title text-uppercase text-muted mb-0">Total</h5>
                        <span class="h2 font-weight-bold mb-0">{{total}}</span>
                    </div>
                    <div class="col-auto">
                        <div class="icon icon-shape bg-gradient-info text-white rounded-circle shadow">
                            <i class="ni ni-chart-bar-32"></i>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>

<div class="table-responsive">
<table class="table align-items-center table-dark">

    <h2 class="d-flex justify-content-center">Images Links</h2>
    <thead class="thead-dark">
        <tr>
            <th scope="col">Image</th>
            <th scope="col">Title</th>
            <th scope="col">Alt</th>

        </tr>
    </thead>
    <tbody>
        {% for item in images %}

        <tr>
            <td>
                <a href="{{item.url}}">
                    <img class="img-extract" src="{{item.url}}" alt="{{item.alt}}">
                </a>
            </td>
            <td>
                {{item.title}}
            </td>
            <td>
                {{item.alt}}
            </td>

        </tr>
        {% endfor %}
    </tbody>
</table>
</div>

{% endblock %}