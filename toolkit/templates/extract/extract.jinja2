{% extends "base.jinja2" %} {% block title %}Extract Headers{% endblock %}{% block name %}Extract {% endblock %}
{% block list %}
<li class="breadcrumb-item active" aria-current="page"> Extract</li>
{%endblock%}
{% block content %}


<div class="row">
    <div class="card-padding card col-2">
        <img class="card-img-top" src="/static/assets/img/theme/headers.png" alt="Headers">
        <div class="card-body">
            <h5 class="card-title">Extract Headers on Page</h5>
            <div style="display: flex; align-items: center; justify-content: center;">
                <a href="/extract/headers" class="btn btn-primary">Headers</a>
            </div>
        </div>
    </div>

    <div class="card-padding card col-2">
        <img class="card-img-top" src="/static/assets/img/theme/links-internal.jpg" alt="Links">
        <div class="card-body">
            <h5 class="card-title">Extract Working Links on Page</h5>
            <div style="display: flex; align-items: center; justify-content: center;">
                <a href="/extract/links" class="btn btn-primary">Links</a>
            </div>
        </div>
    </div>
    <div class="card-padding card col-2">
        <img class="card-img-top" src="/static/assets/img/theme/links.jpg" alt="Links Website">
        <div class="card-body">
            <h5 class="card-title">Extract All Links on Website</h5>
            <div style="display: flex; align-items: center; justify-content: center;">
                <a href="/extract/links/website" class="btn btn-primary">Links Website</a>
            </div>
        </div>
    </div>
     <div class="card-padding card col-2">
        <img class="card-img-top" src="/static/assets/img/theme/capture.jpg" alt="Images Page">
        <div class="card-body">
            <h5 class="card-title">Extract All Images of a Page</h5>
            <div style="display: flex; align-items: center; justify-content: center;">
                <a href="/extract/images" class="btn btn-primary">Images Page</a>
            </div>
        </div>
    </div>

</div>


{% endblock %}