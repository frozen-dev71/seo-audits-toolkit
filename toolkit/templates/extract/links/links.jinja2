{% extends "base.jinja2" %} {% block title %}Extract Links{% endblock %} {% block list %}
<li class="breadcrumb-item"><a href="/extract">Extract</a></li>
<li class="breadcrumb-item"><a href="/extract/links">Links</a></li>
<li class="breadcrumb-item active" aria-current="page">{{id}}</a>
</li>
{%endblock%} {% block content %}

{% for status in links_status %}
<table class="table align-items-center table-flush">

    <h2 class="d-flex justify-content-center">{{status}}</h2>
    <thead class="thead-light">
        <tr>
            <th scope="col">Values</th>
        </tr>
    </thead>
    <tbody>
        {% for item in results[status] %}

        <tr>
            <td>
                <a href="{{item}}">
                    {{item}}
                </a>

            </td>
        </tr>
        {% endfor %}
    </tbody>
</table>
{% endfor %}



{% endblock %}